            "post_process": false
        },
        "convai_pak_manager": {
            "repo": "Conv-AI/Convai-UnrealEngine-PakManager",
            "asset_patterns": [".zip", "plugin", "pak", "manager"],
            "post_process": false
        }
    },
    "google_drive": {
        "convai_reallusion_content": "1bAatTW4vYycDbGLeO1pGILc3OVOOR3je"
